  "license": "MIT",
  "scripts": {
    "server:start": "http-server --port=8080 --cors=*",
    "server:stop": "pkill -f http-server",
    "webhooks:build": "ts-node ./scripts/build-webhooks.ts",
    "webhooks:build:dev": "ts-node ./scripts/build-webhooks.ts local",
    "webhooks:watch": "nodemon --config ./nodemon.webhooks.json",
    "webhooks:start": "yarn server:start & yarn webhooks:watch  && yarn server:stop"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.37",
    "chalk": "^4.1.0",
    "http-server": "^0.12.3",
    "js-yaml": "^4.0.0",
    "nodemon": "^2.0.7",
    "openapi-merge": "^1.0.29",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
