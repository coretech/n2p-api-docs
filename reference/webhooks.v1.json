  "webhooks": {
    "call_answered": {
      "summary": "this is a summary",
      "description": "This event is triggered when a Team Member answers an incoming call or when an outgoing call is answered by the dialed destination number",
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://raw.githubusercontent.com/coretech/n2p-i2-docs/dev/models/webhooks/call_answered.v1.yaml"
                }
              }
            }
          }
        }
      }
    },
    "call_blind_transfer": {
